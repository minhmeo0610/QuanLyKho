                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        ChiTietHoaDon.MaSP, ChiTietHoaDon.MaHD, ChiTietHoaDon.SoLuong, ChiTietHoaDon.DonGia, DoiTac.TenDoiTac, DoiTac.DiaChi, DoiTac.SoDT, HoaDon.NgayLap, HoaDon.KieuHoaDon, LoaiSanPham.TenLoaiSP, 
                         NhanVien.TenNV, SanPham.TenSP, ChiTietHoaDon.SoLuong * SanPham.Gia AS ThanhTien
FROM            ChiTietHoaDon INNER JOIN
                         HoaDon ON ChiTietHoaDon.MaHD = HoaDon.MaHD INNER JOIN
                         DoiTac ON HoaDon.MaDoiTac = DoiTac.MaDoiTac INNER JOIN
                         NhanVien ON HoaDon.MaNV = NhanVien.MaNV INNER JOIN
                         SanPham ON ChiTietHoaDon.MaSP = SanPham.MaSP INNER JOIN
                         LoaiSanPham ON SanPham.MaLoaiSP = LoaiSanPham.MaLoaiSP
WHERE        (ChiTietHoaDon.MaHD IN
                             (SELECT        MaHD
                               FROM            HoaDon AS HoaDon_1
                               WHERE        (MaHD = @MaHD)))</CommandText>
